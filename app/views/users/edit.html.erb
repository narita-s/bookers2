<h2>プロフィール編集</h2>
<%= form_with model: @user do |f| %>
  <label for="user_name">Name</label>
  <%= f.text_field :name, autofocus: true, id:"user_name", placeholder:"名前"%>
  <label for="user_image">ProfileImage</label>
  <%= f.file_field :profile_image, placeholder: "user_image", accept: "image/*" %>
  <label for="user_introduction">Introduction</label>
  <%= f.text_field :introduction, autofocus: true, id:"user_introduction" %>
  <%= f.submit "変更を保存" %>
<% end %>